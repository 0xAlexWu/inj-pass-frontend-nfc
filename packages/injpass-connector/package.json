{
  "name": "@injpass/connector",
  "version": "0.1.0",
  "description": "Lightweight SDK for embedding INJ Pass wallet in dApps",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
