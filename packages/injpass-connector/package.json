  },
  "keywords": [
    "injective",
    "wallet",
    "passkey",
    "webauthn",
    "connector",
    "iframe"
  ],
  "author": "INJ Pass Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/injective/inj-pass"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
